<section id="content" class="home">
		  
	<div class="wrapper">
		<div class="sub-inner-wrapper">
			<div class="inner-wrapper">
				<div class="slides">
				{.repeated section items}

				{.main-image?}  
					<img src="{assetUrl}?format=410w" />
				{.end}

			    {.end}
				</div> <!-- .slides -->
				<div id="text-main-content">
					<div class="box">
					<squarespace:block-field id="main-content" columns="12" />
					</div>
				</div>
				<div class="bottom"></div>
			</div>
		</div>
	
		<div class="other-wrapper">
			<section id="under-main-content">
					<squarespace:block-field id="sub-main-content" columns="12" />
			</section>
		</div>

	</div><!--.wrapper-->

</section> <!-- #content -->