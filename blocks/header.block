<!doctype html>

<html>

  <head>
    
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
	

    <!--INCLUDE SQS SCRIPTS, META TAGS AND USER CONTENT FROM THE CODE INJECTION TAB-->

    {squarespace-headers}

    <!--

      To load up a script, use this syntax in site.region.
      <squarespace:script src="site.js" combo="true" />
	  <squarespace:Script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" combo="false" />
      Helpful tip: For debugging set combo to false.

    -->
  
  </head>

  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
  
      <!--HEADER-->
  
		<header id="header">
	  		<div class="wrapper">
			<!--SITE TITLE OR LOGO-->
	  
			{.section website}
	  
			  <div id="logo">
				<a href="/">
				  {.section logoImageUrl}
				   <img src="{logoImageUrl}?format=original" alt="{siteTitle}" />
				  {.or}
					{siteTitle}
				  {.end}
				</a>
			  </div>
			  
			  <!--SITE TAGLINE-->
	  
			  <!--{.section siteTagLine}<p>{@}</p>{.end}-->
	  
			{.end}
	 			<div id="right">
					<div class="top">
						Make An Appointment
					</div>
					<div class="bottom">
						<div class="left">
							{item|social-button}
							{item|address}
							{item|phone}
						</div>
						<div class="right">
							
						</div>
					</div>
				</div> 

	  
	  		</div>
		  </header>
 			<!--MAIN NAVIGATION-->
	  	  <squarespace:navigation navigationId="mainNav" template="navigation" />
		  <!--CONTENT INJECTION POINT-->
	  
		  <section id="content">    