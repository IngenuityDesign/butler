<!doctype html>

<html>

  <head>
    
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
	

    <!--INCLUDE SQS SCRIPTS, META TAGS AND USER CONTENT FROM THE CODE INJECTION TAB-->

    {squarespace-headers}

    <!--

      To load up a script, use this syntax in site.region.
      <squarespace:script src="site.js" combo="true" />
	  <squarespace:Script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" combo="false" />
      Helpful tip: For debugging set combo to false.

    -->
	
	<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>
	<script>window.jQuery || document.write('<script src="scripts/jquery-2.0.0.min.js"><\/script>')</script>
  	<squarespace:script src="site.js" combo="true" />
  </head>

  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
  
      <!--HEADER-->
  
		<header id="header">
	  		<div class="wrapper">
			<!--SITE TITLE OR LOGO-->
	  		
				<div class="right">
					<div class="top">
						<squarespace:navigation navigationId="appointmentNav" template="appNav" />
					</div> <!-- .top -->
					<div class="bottom">
						<div class="left">
						{.section website}
						{.section location}
							<div class="address">
							{addressLine1}<br>
							{addressLine2}
							</div>
						{.end}
						{.end}
							<div class="social">
								<ul style="margin: 0; padding: 0;>
									<li style="display:inline;"><a style="display: block; float: left;width: 24px; height: 24px; margin-right: 3px;background-image: url(/assets/social.png);" href="#"></a></li>
									<li style="display:inline;"><a style="background-position: 24px 0;display: block; float: left;width: 24px; height: 24px; margin-right: 3px;background-image: url(/assets/social.png);" href="#"></a></li>
									<li style="display:inline;"><a style="background-position: 51px 0;display: block; float: left;width: 24px; height: 24px; margin-right: 3px;background-image: url(/assets/social.png);" href="#"></a></li>
									<li style="display:inline;"><a style="background-position: 80px 0;display: block; float: left;width: 24px; height: 24px; margin-right: 3px;background-image: url(/assets/social.png);" href="#"></a></li>
									<li style="display:inline;"><a style="background-position: 108px 0;display: block; float: left;width: 24px; height: 24px; margin-right: 3px;background-image: url(/assets/social.png);" href="#"></a></li>
								</ul>
								
							</div>
						
						</div> <!-- .left -->
						<div class="right">
							{.section websiteSettings}
							<div class="phone">
							P: {contactPhoneNumber}
							</div>
							<div class="phone">
							F: 714.680.9501
							{.end}
						</div> <!-- .right -->
					</div> <!-- .bottom -->
				</div>  <!-- .right -->
			
			{.section website}
	  
			  <div id="logo">
				<a href="/">
				  {.section logoImageUrl}
				   <img src="{logoImageUrl}?format=original" alt="{siteTitle}" />
				  {.or}
					{siteTitle}
				  {.end}
				</a>
			  </div>
			  
			  <!--SITE TAGLINE-->
	  
			  <!--{.section siteTagLine}<p>{@}</p>{.end}-->
	  
			{.end}

	  
	  		</div>
		  </header>
 		  <!--MAIN NAVIGATION-->
	  	  <squarespace:navigation navigationId="mainNav" template="navigation" />
		  <!--CONTENT INJECTION POINT-->
	   